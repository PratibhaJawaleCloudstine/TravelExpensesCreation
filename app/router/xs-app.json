{
   "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/odata/(.*)$",
      "target": "/odata/$1",
      "destination": "srv-api",
      "authenticationType": "xsuaa"
    },
     {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    },
     {
      "source": "^/createtravelexpensesui/(.*)$",
      "target": "$1",
      "destination": "CreateTravelExpenses-html5-service",
      "csrfProtection": false,
      "authenticationType": "xsuaa"
    }
  ]
}
